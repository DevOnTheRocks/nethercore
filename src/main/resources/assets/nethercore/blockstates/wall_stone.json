{
  "forge_marker": 1,
  "defaults": {
    "transform": "forge:default-block",
    "model": "minecraft:wall_post",
    "textures": {
      "wall": "nethercore:blocks/nether_stone_cobble"
    }
  },
  "variants": {
    "east": {"true": {}, "false": {}},
    "north": {"true": {}, "false": {}},
    "south": {"true": {}, "false": {}},
    "up": {"true": {}, "false": {}},
    "west": {"true": {}, "false": {}},
    "variant": {
      "stone": {
        "textures": {
          "wall": "nethercore:blocks/nether_stone"
        }
      },
      "cobblestone": {
        "textures": {
          "wall": "nethercore:blocks/nether_stone_cobble"
        }
      },
      "brick": {
        "textures": {
          "wall": "minecraft:blocks/nether_brick"
        }
      }
    }
  },
  "multipart": [
    {
      "when": { "up": "true" },
      "apply": { "model": "minecraft:wall_post" }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "minecraft:block/wall_side", "uvlock": true }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "minecraft:block/wall_side", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "minecraft:block/wall_side", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "minecraft:block/wall_side", "y": 270, "uvlock": true }
    }
  ]
}
